{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "globals": {
        "fetch": false
    },
    "rules": {
        "prettier/prettier": ["error"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]  }]
    }
}
